script de creacion de la tabla

CREATE TABLE PACIENTE(
	IDPACIENTE	INTEGER PRIMARY KEY NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
	NOMBRE		VARCHAR(50),
	FECHANAC	DATE,
	ESTATURA	INTEGER,
	SEXO		CHAR		
);